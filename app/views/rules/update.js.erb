<% if @organization && @rule %>
  $('#new-criteria').modal('hide');
  $('#organization_club').html('<%= j render partial: "list_rules", locals: {rules: @rules} %>');
  $('.modal-backdrop').remove()
  $('body').removeClass('modal-open');
<% end %>
$('.container').append('<%= j render "shared/flash_warning" %>')
$('.notify').slideUp(3000);
